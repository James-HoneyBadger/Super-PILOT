                        self.log_output("Turtle trace enabled")
                    elif state in ("OFF", "FALSE", "0"):
                        self.turtle_trace = False
                        self.log_output("Turtle trace disabled")
                else:
                    self.turtle_trace = not self.turtle_trace
                    self.log_output(
