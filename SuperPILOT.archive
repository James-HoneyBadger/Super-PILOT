                            y_frac = max(
                                0.0,
                                min(
                                    (cy - (0.5 * canvas.winfo_height()))
                                    / max(total_h, 1),
                                    1.0,
                                ),
                            )
                            canvas.xview_moveto(x_frac)
                            canvas.yview_moveto(y_frac)
                            self.log_output("View fit to drawing bounds")
