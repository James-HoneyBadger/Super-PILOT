                    else []
                )
                self.log_output(f"DEBUGLINES {len(meta)} lines")
                for i, m in enumerate(meta[:10]):
                    self.log_output(
                        f"  {i}: ({m['x1']:.1f},{m['y1']:.1f})->({m['x2']:.1f},{m['y2']:.1f}) {m['color']} w={m['width']} {m['style']}"
                    )
                if len(meta) > 10:
                    self.log_output("  ...")

            elif cmd == "HUD":
                if self.turtle_graphics:
