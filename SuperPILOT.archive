        try:
            self.editor.config(
                insertbackground=self.colors["primary"],
                highlightcolor=self.colors["primary"],
            )
            self.output_text.config(highlightcolor=self.colors["primary"])
            self.status_label.config(fg=self.colors["text_primary"])
