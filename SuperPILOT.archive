                    bbox = canvas.bbox("all")
                    if bbox:
                        total_w = bbox[2] - bbox[0]
                        total_h = bbox[3] - bbox[1]
                        if total_w > 0 and total_h > 0:
                            cx = bbox[0] + total_w / 2
                            cy = bbox[1] + total_h / 2
                            x_frac = max(
                                0.0,
                                min(
                                    (cx - (0.5 * canvas.winfo_width()))
                                    / max(total_w, 1),
                                    1.0,
                                ),
