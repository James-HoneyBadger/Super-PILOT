                var_name = parts[2] if len(parts) > 2 else sensor_type

                if sensor_type == "DISTANCE":
                    value = self.robot.read_distance_sensor()
                    self.variables[var_name] = value
                    self.log_output(f"Distance sensor: {value} cm")
